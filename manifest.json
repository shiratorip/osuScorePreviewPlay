{
  "name": "Osu score preview play",
  "description": "Add preview play button to osu scores on your page",
  "version": "1.0",
  "manifest_version": 3,
  "author": "shiratorip",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://osu.ppy.sh/users/*",
        "https://osu.ppy.sh/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": [
    "*://*.ppy.sh/*"
  ]
  
}
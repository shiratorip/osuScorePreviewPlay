{
  "name": "Osu score preview play",
  "description": "Add preview play button to osu scores on your page",
  "version": "1.0",
  "manifest_version": 3,
  "author": "shiratorip",
  "author_email": "shiratorip.contact@gmail.com",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.ppy.sh/*"],
      "js": [
        "scripts/content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}